<p id="notice"><%= notice %></p>

<h1>User Seasons</h1>

<table>
  <thead>
    <tr>
      <th>Users</th>
      <th>Seasons</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_seasons.each do |user_season| %>
      <tr>
        <td><%= user_season.users %></td>
        <td><%= user_season.seasons %></td>
        <td><%= link_to 'Show', user_season %></td>
        <td><%= link_to 'Edit', edit_user_season_path(user_season) %></td>
        <td><%= link_to 'Destroy', user_season, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Season', new_user_season_path %>
